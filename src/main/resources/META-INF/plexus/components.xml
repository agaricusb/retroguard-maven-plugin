<component-set>
    <components>
        <component>
            <role>com.digitalreasoning.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>compiler</role-hint>
            <implementation>com.digitalreasoning.mojo.retroguard.modifier.CompilerObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.digitalreasoning.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>surefire</role-hint>
            <implementation>com.digitalreasoning.mojo.retroguard.modifier.SurefireObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.digitalreasoning.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>jar</role-hint>
            <implementation>com.digitalreasoning.mojo.retroguard.modifier.JarObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.digitalreasoning.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>war</role-hint>
            <implementation>com.digitalreasoning.mojo.retroguard.modifier.WarObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.digitalreasoning.mojo.retroguard.RevealingExecutionListener</role>
            <role-hint>default</role-hint>
            <implementation>com.digitalreasoning.mojo.retroguard.RevealingExecutionListener</implementation>
            <isolated-realm>false</isolated-realm>
            <requirements>
                <requirement>
                    <role>com.digitalreasoning.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
                    <field-name>modifiers</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.artifact.factory.ArtifactFactory</role>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.ArtifactResolver</role>
                </requirement>
            </requirements>
        </component>
        <component>
            <role>org.apache.maven.AbstractMavenLifecycleParticipant</role>
            <role-hint>obfuscated</role-hint>
            <implementation>com.digitalreasoning.mojo.retroguard.ObfuscatingMavenLifecycleParticipant</implementation>
            <requirements>
              <requirement>
                <role>com.digitalreasoning.mojo.retroguard.RevealingExecutionListener</role>
              </requirement>
            </requirements>
        </component>
        <component>
          <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
          <role-hint>obfuscated.jar</role-hint>
          <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
          <configuration>
            <lifecycles>
              <lifecycle>
                <id>default</id>
                <phases>
                  <process-resources>org.apache.maven.plugins:maven-resources-plugin:2.4.3:resources</process-resources>
                  <compile>org.apache.maven.plugins:maven-compiler-plugin:2.3.2:compile</compile>
                  <process-test-resources>org.apache.maven.plugins:maven-resources-plugin:2.4.3:testResources</process-test-resources>
                  <test-compile>org.apache.maven.plugins:maven-compiler-plugin:2.3.2:testCompile</test-compile>
                  <test>org.apache.maven.plugins:maven-surefire-plugin:2.7.2:test</test>
                  <package>org.apache.maven.plugins:maven-jar-plugin:2.3.1:jar,com.digitalreasoning.mojo:retroguard-maven-plugin:jarObfuscate</package>
                  <install>org.apache.maven.plugins:maven-install-plugin:2.3.1:install</install>
                  <deploy>org.apache.maven.plugins:maven-deploy-plugin:2.5:deploy</deploy>
                </phases>
              </lifecycle>
            </lifecycles>
          </configuration>
        </component>
        <component>
          <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
          <role-hint>obfuscated.war</role-hint>
          <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
          <configuration>
            <lifecycles>
              <lifecycle>
                <id>default</id>
                <phases>
                  <process-resources>org.apache.maven.plugins:maven-resources-plugin:2.4.3:resources</process-resources>
                  <compile>org.apache.maven.plugins:maven-compiler-plugin:2.3.2:compile</compile>
                  <process-test-resources>org.apache.maven.plugins:maven-resources-plugin:2.4.3:testResources</process-test-resources>
                  <test-compile>org.apache.maven.plugins:maven-compiler-plugin:2.3.2:testCompile</test-compile>
                  <test>org.apache.maven.plugins:maven-surefire-plugin:2.7.2:test</test>
                  <package>org.apache.maven.plugins:maven-war-plugin:2.1.1:war,com.digitalreasoning.mojo:retroguard-maven-plugin:warObfuscate</package>
                  <install>org.apache.maven.plugins:maven-install-plugin:2.3.1:install</install>
                  <deploy>org.apache.maven.plugins:maven-deploy-plugin:2.5:deploy</deploy>
                </phases>

              </lifecycle>
            </lifecycles>
          </configuration>
        </component>
        <component>
          <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
          <role-hint>obfuscated.jar</role-hint>
          <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
          <configuration>
            <type>obfuscated.jar</type>
            <extension>jar</extension>
            <language>java</language>
            <addedToClasspath>true</addedToClasspath>
          </configuration>
        </component>
        <component>
          <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
          <role-hint>unobfuscated.jar</role-hint>
          <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
          <configuration>
            <type>unobfuscated.jar</type>
            <extension>jar</extension>
            <language>java</language>
            <addedToClasspath>true</addedToClasspath>
            <classifier>unobfuscated</classifier>
          </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>obfuscated.war</role-hint>
            <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
            <configuration>
                <type>obfuscated.war</type>
                <extension>war</extension>
                <includesDependencies>true</includesDependencies>
                <language>java</language>
                <addedToClasspath>false</addedToClasspath>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>unobfuscated.war</role-hint>
            <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
            <configuration>
                <type>unobfuscated.war</type>
                <extension>war</extension>
                <includesDependencies>true</includesDependencies>
                <language>java</language>
                <addedToClasspath>false</addedToClasspath>
                <classifier>unobfuscated</classifier>
            </configuration>
        </component>
        <component>
          <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
          <role-hint>retroguard-spec</role-hint>
          <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
          <configuration>
            <type>retroguard-spec</type>
            <extension>rgs</extension>
            <language>retroguard</language>
            <addedToClasspath>false</addedToClasspath>
          </configuration>
        </component>
    </components>
</component-set>