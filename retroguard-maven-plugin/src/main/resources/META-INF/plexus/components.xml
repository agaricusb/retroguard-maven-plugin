<component-set>
    <components>
        <component>
            <role>com.peachjean.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>compiler</role-hint>
            <implementation>com.peachjean.mojo.retroguard.modifier.CompilerObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.peachjean.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>surefire</role-hint>
            <implementation>com.peachjean.mojo.retroguard.modifier.SurefireObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.peachjean.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
            <role-hint>jar</role-hint>
            <implementation>com.peachjean.mojo.retroguard.modifier.JarObfuscationMojoExecutionModifier</implementation>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>com.peachjean.mojo.retroguard.RevealingExecutionListener</role>
            <role-hint>default</role-hint>
            <implementation>com.peachjean.mojo.retroguard.RevealingExecutionListener</implementation>
            <isolated-realm>false</isolated-realm>
            <requirements>
                <requirement>
                    <role>com.peachjean.mojo.retroguard.ObfuscationMojoExecutionModifier</role>
                    <field-name>modifiers</field-name>
                </requirement>
            </requirements>
        </component>
        <component>
            <role>org.apache.maven.AbstractMavenLifecycleParticipant</role>
            <role-hint>obfuscated</role-hint>
            <implementation>com.peachjean.mojo.retroguard.ObfuscatingMavenLifecycleParticipant</implementation>
            <requirements>
              <requirement>
                <role>com.peachjean.mojo.retroguard.RevealingExecutionListener</role>
              </requirement>
            </requirements>
        </component>
        <component>
          <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
          <role-hint>obfuscated.jar</role-hint>
          <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
          <configuration>
            <lifecycles>
              <lifecycle>
                <id>default</id>
                <phases>
                  <initialize>com.peachjean.mojo:retroguard-maven-plugin:init</initialize>
                  <process-resources>org.apache.maven.plugins:maven-resources-plugin:resources</process-resources>
                  <compile>org.apache.maven.plugins:maven-compiler-plugin:compile</compile>
                  <process-test-resources>org.apache.maven.plugins:maven-resources-plugin:testResources</process-test-resources>
                  <test-compile>org.apache.maven.plugins:maven-compiler-plugin:testCompile</test-compile>
                  <test>org.apache.maven.plugins:maven-surefire-plugin:test</test>
                  <package>org.apache.maven.plugins:maven-jar-plugin:jar,com.peachjean.mojo:retroguard-maven-plugin:obfuscate</package>
                  <install>org.apache.maven.plugins:maven-install-plugin:install</install>
                  <deploy>org.apache.maven.plugins:maven-deploy-plugin:deploy</deploy>
                </phases>
              </lifecycle>
            </lifecycles>
          </configuration>
        </component>
        <component>
          <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
          <role-hint>obfuscated.jar</role-hint>
          <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
          <configuration>
            <type>obfuscated</type>
            <extension>jar</extension>
            <language>java</language>
            <addedToClasspath>true</addedToClasspath>
          </configuration>
        </component>
        <component>
          <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
          <role-hint>unobfuscated.jar</role-hint>
          <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
          <configuration>
            <type>unobfuscated</type>
            <extension>jar</extension>
            <language>java</language>
            <addedToClasspath>true</addedToClasspath>
            <classifier>unobfuscated</classifier>
          </configuration>
        </component>
        <component>
          <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
          <role-hint>retroguard-spec</role-hint>
          <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
          <configuration>
            <type>retroguard-spec</type>
            <extension>rgs</extension>
            <language>retroguard</language>
            <addedToClasspath>false</addedToClasspath>
          </configuration>
        </component>
    </components>
</component-set>